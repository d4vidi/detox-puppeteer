{
  "name": "detox-puppeteer",
  "version": "3.0.5",
  "main": "build/PuppeteerDriver.js",
  "private": false,
  "scripts": {
    "test": "detox test --configuration simple"
  },
  "dependencies": {
    "lodash": "^4.17.19",
    "puppeteer": "^8.0.0",
    "tslib": "^2.2.0",
    "xvfb": "^0.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.8.4",
    "@types/detox": "^14.5.2",
    "@types/jest": "^25.0.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "detox": "^17.0.0",
    "jest": "^25.2.5",
    "prettier": "^1.19.0",
    "typescript": "4.2.4"
  },
  "detox": {
    "artifacts": {
      "plugins": {
        "uiHierarchy": "enabled"
      }
    },
    "behavior": {
      "init": {
        "launchApp": false
      }
    },
    "configurations": {
      "simple": {
        "binaryPath": "http://localhost:8889/",
        "type": "./build/PuppeteerDriver.js",
        "device": {
          "defaultViewport": {
            "width": 375,
            "height": 712
          }
        },
        "name": "puppeteer"
      }
    },
    "test-runner": "jest"
  }
}
